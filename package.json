{
  "name": "translate-cli",
  "version": "1.0.0",
  "description": "A CLI tool to batch translate text into multiple target languages",
  "main": "dist/presentation/cli/cli.js",
  "bin": {
    "translate": "./dist/presentation/cli/cli.js"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node dist/presentation/cli/cli.js",
    "dev": "ts-node src/presentation/cli/cli.ts",
    "lint": "eslint src",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "cli",
    "translate",
    "batch",
    "google-translate"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "https-proxy-agent": "^7.0.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/node": "^20.19.25",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.0",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.2"
  }
}
